type
Student {
	str name;
	int marks;
}
endtype

int main() {
decl
	Student stu;
	int t;
enddecl
begin
	t = initialize();
	t = exposcall("Write", -2, "Pre alloc", 0);
	stu = alloc();
	t = exposcall("Write", -2, "Post alloc", 0);
	stu.name = "Student";
	stu.marks = 100;
	t = exposcall("Write", -2, stu.name, 0);
	t = exposcall("Write", -2, stu.marks, 0);
	free(stu);
	return 0;
end
}
